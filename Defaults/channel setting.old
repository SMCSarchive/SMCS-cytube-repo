body {
     position: relative; /* or absolute, fixed, etc., depending on your layout */
     top: 350px; /* Moves the banner down by 50px from its normal position */

     background-image:
     url(https://pbs.twimg.com/media/Gi94PwwXkAAqdCx?format=jpg&name=large), /* Banner */
     url(https://pbs.twimg.com/media/Gi7ZoT4WsAAnc5_?format=png&name=small); /* Repeating Background */
    
     background-position:
     0 0, /* Banner */
     0 0; /* Repeating Background */
    
     background-size:
     100%, /* Banner */
     20%; /* Repeating Background */
    
     background-repeat: no-repeat, repeat;
}

body .navbar-header::before {
     position: relative; /* or absolute, fixed, etc., depending on your layout */
     top: 0px; /* Moves the banner down by 50px from its normal position */
     float: left;
     padding: 15px;
     font-size: 18px;
     line-height: 20px;
     height: 50px;
     content: "Saturday Morning Cartoon Show";
     font-family: 'Comic Sans MS', cursive;
     background-image:
     url(https://pbs.twimg.com/media/Gi7ZoT4WsAAnc5_?format=png&name=small); /* Banner */
     background-position:
     0px; /* Banner */
     background-size: 
     100%; /* Banner */
     background-repeat: no-repeat;
     background-color: black;
}
body .navbar-brand {
     font-family: 'Comic Sans MS', cursive;
     background-color: black;
}
body .navbar-nav>li>a {
     font-family: 'Comic Sans MS', cursive;
     background-color: black;
}
body .navbar-text {
     float: right top;
     padding: 0px;
     font-size: 18px;
     line-height: 20px;
     height: 20px;
     font-family: 'Comic Sans MS', cursive;
     background-color: black;
}
body .navbar-collapse {background-color: black;}
body .linewrap {
     font-family: 'Comic Sans MS', sans-serif;
     background-color: black;
}
body #chatwrap {
     font-family: 'Comic Sans MS', cursive;
     background-color: black;
}
body .embed-responsive-item {
     background-image:
     url(https://pbs.twimg.com/media/Gi_1zHUWkAAqyuB?format=jpg&name=small); /* No Signal */
     background-position:
     0px 0px; /* No Signal */
     background-size:
     100%; /* No Signal */
     background-repeat: no-repeat;
     background-color: black;
}

.container {
    position: relative; /* or absolute, fixed, etc., depending on your layout */
    top: 100%; /* Moves the banner down by 50px from its normal position */

    /* URLs for your images */
    background-image: 
    url(https://pbs.twimg.com/media/GjOKpakW8AAU86x?format=jpg&name=medium), /* Saturday */
    url(https://pbs.twimg.com/media/GjOKjuvX0AATx25?format=jpg&name=medium); /* Sunday */
    
    /* Size of each image */
    background-size: 50%, 50%;
    
    /* Position the images side by side */

    background-position: left top 60px, right top 60px;
    
    /* Ensure the container is wide enough to show both images */
    width: 100%; /* 100% * 2 */
    height: 900px;
    
    /* Prevent repetition of the images */
    background-repeat: no-repeat;
    
    /* Optional: if you want the container to fit its content */
    display: inline-block;
}